<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8" />
    <title><%- title %></title>
    <% styles.forEach(function(s){ %> <%- s %> <% }); %>
    <% defaultStyles.forEach(function(s){ %> <%- s %> <% }); %>
  </head>
  <body>
    <% defaultScripts.forEach(function(s){ %> <%- s %> <% }); %>
    
    <%- content %>
    
    <% scripts.forEach(function(s){ %> <%- s %> <% }); %>
  </body>
</html>
